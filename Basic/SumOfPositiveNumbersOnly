let numbers = [-1, 1, 2, 3, -5, -6, -7, 8, 4];

let sum = 0;
let i = 0;
numbers[i]
while(i<numbers.length){
    if(numbers[i]>0){
        sum += numbers[i] //sum = sum + particular number in array
    }
    i++;
}

console.log("Sum of all the positive numbers is "+sum);

console.log(`Sum of all the positive numbers is ${sum}`);